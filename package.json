{
  "name": "cs-devops-agent",
  "version": "1.0.0",
  "description": "Code Studio CS_DevOpsAgent Agent - DevOps automation system with intelligent branching and VS Code integration",
  "type": "module",
  "main": "src/cs-devops-agent-worker.js",
  "scripts": {
    "start": "./start-devops-session.sh",
    "devops": "./start-devops-session.sh",
    "devops:new": "node src/session-coordinator.js create-and-start",
    "devops:list": "node src/session-coordinator.js list",
    "devops:run": "echo 'Usage: npm run devops:run <session-id>' && echo 'Example: npm run devops:run ac7c-6220' && exit 1",
    "agent": "node src/cs-devops-agent-worker.js",
    "agent:session": "node src/session-coordinator.js start",
    "setup": "node src/setup-cs-devops-agent.js",
    "worktree": "node src/worktree-manager.js",
    "session": "node src/session-coordinator.js",
    "session:create": "node src/session-coordinator.js create-and-start",
    "session:list": "node src/session-coordinator.js list",
    "claude": "node src/claude-session-manager.js",
    "claude:start": "node src/claude-session-manager.js start",
    "claude:list": "node src/claude-session-manager.js list",
    "test": "jest test_cases/",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:worktree": "jest test_cases/worktree/",
    "test:e2e": "./test_scripts/test-multi-agent-e2e.sh",
    "test:push-behind": "jest test_scripts/push_behind_spec.js",
    "test:debug": "./debug-failing-tests.sh"
  },
  "keywords": [
    "git",
    "automation",
    "commit",
    "workflow",
    "vscode",
    "branching",
    "development-tools"
  ],
  "author": "Sachin Dev Duggal",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "execa": "^7.1.1"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secondbrainAI-limited/code_studio_cs-devops-agentAgent.git"
  },
  "bugs": {
    "url": "https://github.com/secondbrainAI-limited/code_studio_cs-devops-agentAgent/issues"
  },
  "homepage": "https://github.com/secondbrainAI-limited/code_studio_cs-devops-agentAgent#readme"
}
